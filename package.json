{
  "name": "webclient",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-ga": "^2.3.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-timeago": "^4.1.2",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.21.1",
    "enzyme": "^3.11.0",
    "formik": "^2.2.6",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-timeago": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@types/chance": "^1.1.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "chance": "^1.1.7",
    "react-app-rewired": "^2.1.8"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "docker exec -it six__webclient sh -c '$(yarn bin)/react-app-rewired test'",
    "eject": "react-scripts eject",
    "sh": "docker exec -it six__webclient sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "rootDir": ".",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "moduleNameMapper": {
      "_helpers/(.*)": "<rootDir>/src/components/helpers/$1",
      "_slices/(.*)": "<rootDir>/src/components/slices/$1",
      "_views/(.*)": "<rootDir>/src/components/views/$1",
      "_routes/(.*)": "<rootDir>/src/components/routes/$1",
      "_routers/(.*)": "<rootDir>/src/components/routers/$1",
      "_layouts/(.*)": "<rootDir>/src/components/layouts/$1",
      "_store/(.*)": "<rootDir>/src/store/$1.ts",
      "_services/(.*)": "<rootDir>/src/services/$1",
      "_base/(.*)": "<rootDir>/src/$1"
    }
  }
}
